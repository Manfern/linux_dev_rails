<h1>New Post</h1>

<%=  render "form" %>

<%= form_for :post, url: posts_path  do |f| %>
<p>
	<%= f.label :title  %>
	<%= f.text_field :title  %>
</p>
<p>
	<%= f.label :content  %>
	<%= f.text_field :content  %>
</p>
<p>
	<%= %>
	<%= f.submit%>
</p>

<%= form_for @post, url: posts_path, html: { multipart: true } do |form| %>
<%= form.file_field :image %>
<% end %>